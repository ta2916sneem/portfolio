   }\n    },\n    clipboard: { matchVisual: false },\n    syntax: true\n};\n\nconst formats = [\n    \"align\", \"background\", \"blockquote\", \"bold\",\n    \"code\", \"color\", \"direction\", \"font\", \"formula\",\n    \"header\", \"image\", \"indent\", \"italic\", \"link\",\n    \"list\", \"script\", \"size\", \"strike\", \"table\", \"underline\",\n    \"video\", \"code-block\"\n];\n\nconst Editor = forwardRef<ReactQuill, Props>(({content}, editorRef) => {\n\n    const [quillText, setQuillText] = useState<string>(content);\n\n    const handleChange = (val:any) => {\n        setQuillText(val);\n    }\n\n    return (\n        <div className={\"w-full mb-8 shadow-drop\"}>\n            <ReactQuill\n                value={quillText}\n                onChange={(val) => handleChange(val)}\n                theme={\"snow\"}\n                modules={modules}\n                formats={formats}\n                ref={editorRef}\n                defaultValue={content}\n                placeholder={\"Write Something...\"}\n            />\n        </div>\n    )\n});\n\n\nexport default Editor;","/Users/raiyanrazi/WebstormProjects/portfolio/portfolio-react/src/paths.ts",[],"/Users/raiyanrazi/WebstormProjects/portfolio/portfolio-react/src/sections/About.tsx",[],"/Users/raiyanrazi/WebstormProjects/portfolio/portfolio-react/src/components/Level.tsx",[],"/Users/raiyanrazi/WebstormProjects/portfolio/portfolio-react/src/components/Navbar.tsx",["417","418","419","420"],"import React,{FC} from \"react\";\nimport {Link, withRouter} from \"react-router-dom\";\nimport paths from \"../paths\";\nimport {downloadResume} from '../factory/actions/resumeActions'\nimport {logout} from \"../factory/actions/userActions\";\nimport {RootStateOrAny, useDispatch, useSelector} from \"react-redux\";\nimport {isAdmin} from \"../auth\";\n\nconst Navbar: FC = () => {\n\n    const userLogin = useSelector((state:RootStateOrAny) => state.userLogin);\n    const {userInfo} = userLogin;\n    const dispatch = us